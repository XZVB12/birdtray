{
  "problemMatcher": [
    {
      "owner": "checkTranslation",
      "pattern": [
        {
          "regexp": "^\\[(.*)\\]\\s+(.*):(\\d+):(\\d+):\\s+(.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 5
        }
      ]
    }
  ]
}
